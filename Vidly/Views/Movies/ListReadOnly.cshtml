@model List<Vidly.Models.Movie>

<h2>Movies</h2>

<table class="table table-striped table-bordered table-hover">
    <thead>
    <tr>
        <td>Name</td>
        <td>Genre</td>
        <td>Release Date</td>
        <td>Stock Qty.</td>



    </tr>
    </thead>
    <tbody>
    @foreach (var movie in Model)
    {
        <tr>
            <td>@movie.Name</td>
            <td>@movie.Genre.Name</td>
            <td>@movie.ReleaseDate.ToString("D")</td>
            <td>@movie.StockQty</td>



        </tr>


    }
    </tbody>
</table>